        {"name": "v", "type": "int", "doc": "Value schema version for the given topic."},
        {"name": "s", "type": "string", "doc": "Source ID of the sent data."},
        {"name": "d", "type": {"type": "array", "items": "bytes", "doc": "Binary serialized Avro records."}, "doc": "Collected data. This should just contain the value records."}
    ]
